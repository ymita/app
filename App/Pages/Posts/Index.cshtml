@page
@model App.Pages.Posts.IndexModel
@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<form asp-page="../Dashboard/Posts/Index" method="post">
    <button type="submit">send</button>
</form>

@{
    if (@Model.Posts.Count > 0) {
        for (int i = 0; i < @Model.Posts.Count; i++)
        {
    <div>
        <a href="/posts/@Model.Posts[i].Id">@Model.Posts[i].Title</a>
    </div>
        }
    }
}
